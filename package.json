{
    "name": "marketivo-frontend",
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "ng": "ng",
        "start": "npm run pre-start && ng serve",
        "start:no-check": "ng serve",
        "build": "npm run pre-build && npx ng run marketivo-frontend:ngsscbuild:production",
        "build:no-check": "npx ng run marketivo-frontend:ngsscbuild:production",
        "watch": "npm run pre-start && ng build --watch --configuration development",
        "pre-start": "scripts\\check-code-quality.bat",
        "pre-start:js": "node scripts/check-code-quality.js",
        "pre-build": "scripts\\check-code-quality.bat",
        "pre-build:js": "node scripts/check-code-quality.js",
        "format": "prettier --write \"**/*.{js,mjs,ts,mts,d.ts,html}\" --cache",
        "format:check": "prettier --check \"**/*.{js,mjs,ts,mts,d.ts,html}\"",
        "lint": "eslint \"**/*.{js,mjs,ts,mts}\" --fix",
        "lint:check": "eslint \"**/*.{js,mjs,ts,mts}\"",
        "lint:show": "eslint \"**/*.{js,mjs,ts,mts}\" --fix --format=table",
        "code:fix": "npm run format && npm run lint",
        "code:check": "npm run format:check && npm run lint:check",
        "code:check:strict": "npm run format:check && npm run lint:check --max-warnings 0",
        "test": "ng test",
        "dev": "npm run pre-start && ng serve --ssl --ssl-cert certs\\localhost.crt --ssl-key certs\\localhost.key --configuration=development",
        "dev:no-check": "ng serve --ssl --ssl-cert certs\\localhost.crt --ssl-key certs\\localhost.key --configuration=development",
        "stage": "npm run pre-start && ng serve --ssl --ssl-cert certs\\localhost.crt --ssl-key certs\\localhost.key --configuration=staging",
        "stage:no-check": "ng serve --ssl --ssl-cert certs\\localhost.crt --ssl-key certs\\localhost.key --configuration=staging",
        "prod": "npm run pre-start && ng serve --ssl --ssl-cert certs\\localhost.crt --ssl-key certs\\localhost.key --configuration=production",
        "prod:no-check": "ng serve --ssl --ssl-cert certs\\localhost.crt --ssl-key certs\\localhost.key --configuration=production"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^20",
        "@angular/common": "^20",
        "@angular/compiler": "^20",
        "@angular/core": "^20",
        "@angular/forms": "^20",
        "@angular/platform-browser": "^20",
        "@angular/platform-browser-dynamic": "^20",
        "@angular/router": "^20",
        "@auth0/auth0-angular": "^2.2.3",
        "@fortawesome/fontawesome-free": "^6.7.2",
        "@ngrx/effects": "^20.0.0",
        "@ngrx/store": "^20.0.0",
        "@ngrx/store-devtools": "^20.0.0",
        "@ngx-translate/core": "^17.0.0",
        "@ngx-translate/http-loader": "^17.0.0",
        "@primeuix/themes": "^1.2.1",
        "@stripe/stripe-js": "^7.8.0",
        "@tailwindcss/postcss": "^4.1.11",
        "angular-server-side-configuration": "^20.0.0",
        "flag-icons": "^7.5.0",
        "fluentvalidation-ts": "^5.0.0",
        "npm-check-updates": "^18.0.1",
        "primeclt": "^0.1.5",
        "primeicons": "^7.0.0",
        "primeng": "^20",
        "rxjs": "~7.8.2",
        "tailwindcss-primeui": "^0.6.1",
        "tslib": "^2.8.1",
        "zone.js": "~0.15.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^20",
        "@angular-eslint/eslint-plugin": "^20.1.1",
        "@angular-eslint/eslint-plugin-template": "^20.1.1",
        "@angular/cli": "^20",
        "@angular/compiler-cli": "^20",
        "@eslint/js": "^9.33.0",
        "@types/jasmine": "~5.1.0",
        "@typescript-eslint/eslint-plugin": "^8.39.1",
        "@typescript-eslint/parser": "^8.39.1",
        "autoprefixer": "^10.4.20",
        "eslint": "^9.30.1",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-prettier": "^5.5.1",
        "jasmine-core": "~5.8.0",
        "karma": "~6.4.4",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.1",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "postcss": "^8.5.6",
        "prettier": "^3.6.2",
        "tailwindcss": "^4.1.11",
        "typescript": "~5.8.3"
    }
}
